{
  "name": "dots-wrapper",
  "version": "2.6.0",
  "description": "Digital Ocean API V2 wrapper. TypeScript - Javascript",
  "main": "dist/digitalOcean.js",
  "types": "dist/digitalOcean.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "rm -rf ./dist && tsc",
    "test:unit": "ts-node node_modules/.bin/jasmine --config=./src/spec/support/jasmine.json",
    "test:coverage": "jest",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pjpimentel/dots.git"
  },
  "author": "Pedro J. Pimentel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pjpimentel/dots/issues"
  },
  "homepage": "https://github.com/pjpimentel/dots",
  "dependencies": {
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.16",
    "@types/node": "^12.6.9",
    "axios-mock-adapter": "^1.17.0",
    "babel-jest": "^24.8.0",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [],
    "reporter": [
      "text-summary",
      "text",
      "html",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true,
    "cache": false
  }
}
